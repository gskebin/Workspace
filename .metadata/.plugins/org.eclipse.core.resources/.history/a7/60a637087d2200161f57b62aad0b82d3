package parkjugod;

import java.util.Scanner;

public class Ex05p {

	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);

		int a;
		int b=0;
		while (true) {
			a = scan.nextInt();
			if (a == 0) {
				break;
			}if(a>0){
				b=b+a;
				System.out.println(b);
			}else if(a<0){
				System.out.println(" ");
			}
				
			
		}
	}
}
