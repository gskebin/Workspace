package parkjugod;

public class Ex20 {

	public static void main(String[] args) {
		
		
		int a = 0;
		// 대대
		for (int i = 1; i <= 3; i++) {
			
			// 중대
			for (int j = 1; j <= 3; j++) {
				
				// 소대
				for (int j2 = 1; j2 <= 3; j2++) {
					
					// 분대
					for (int k = 1; k <= 10; k++) {
						a++;
					}
					
				}
				
				
			}
			
			
		}
		
	}
}
